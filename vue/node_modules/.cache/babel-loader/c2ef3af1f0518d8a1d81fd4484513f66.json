essage({\r\n                showClose: true,\r\n                message:\"开户行:\"+data.bankName+\" \\n银行卡类型:\"+data.cardTypeName,\r\n              });\r\n              this.$router.push({ path: \"../WaitView1\" });\r\n            \r\n\r\n          }\r\n          else if (err){\r\n            this.$message({\r\n              showClose: true,\r\n              message: \"银行卡不正确\",\r\n              type: \"error\",\r\n            });\r\n\r\n          }\r\n        })\r\n\r\n      }\r\n    },\r\n\r\n  },\r\n   mounted() {\r\n    this.startIdleTimer();\r\n    document.addEventListener('keydown', this.resetIdleTimer);} ,\r\n  beforeDestroy() {\r\n    this.clearIdleTimer();\r\n    document.removeEventListener('keydown', this.resetIdleTimer); // 在组件销毁前移除事件监听器，避免内存泄漏\r\n  },\r\n};\r\n</script>\r\n\r\n<style >\r\n.main{\r\n  display:flex;\r\n  justify-content: space-between;\r\n  width: 100%;\r\n  height: 780px;\r\n  background: url(\"../../assets/中国银行图片.png\")  no-repeat center fixed;\r\n  background-size: cover;\r\n}\r\n.left{\r\n  padding-top: 300px;\r\n    box-sizing: border-box;\r\n  width:300px;\r\n  height:750px;\r\n\r\n}\r\n.center{\r\n  padding-top: 50px;\r\n    box-sizing: border-box;\r\n  width:900px;\r\n  height:750px;\r\n  text-align: center;\r\n}\r\n.right{\r\n  padding-top: 300px;\r\n  box-sizing: border-box;\r\n  text-align: right;\r\n  width:300px;\r\n  height:750px;\r\n  \r\n}\r\n.el-button{\r\n  font-size: 30px;\r\n  color:black;\r\n  background-color: white;\r\n}\r\n\r\n</style>"],"mappings":";AAyBA,OAAAA,QAAA;AACA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,KAAA;MAAA;MACAC,YAAA,EAAAC,IAAA,CAAAC,GAAA;IACA;EACA;EACAC,OAAA;IACAC,eAAA;MACA;MACA,MAAAF,GAAA,GAAAD,IAAA,CAAAC,GAAA;MACA,IAAAA,GAAA,QAAAF,YAAA;QAAA;QACA,KAAAK,cAAA;MACA;QACAC,YAAA,MAAAP,KAAA;QACA,KAAAA,KAAA,GAAAQ,UAAA;UACA,KAAAC,OAAA,CAAAC,IAAA;QACA;MACA;MACA,KAAAT,YAAA,GAAAE,GAAA;IACA;IACAG,eAAA;MACA;MACA,KAAAN,KAAA,GAAAQ,UAAA;QACA,KAAAC,OAAA,CAAAC,IAAA;MACA;IACA;IACAC,eAAA;MACA;MACAJ,YAAA,MAAAP,KAAA;IACA;IACAY,W