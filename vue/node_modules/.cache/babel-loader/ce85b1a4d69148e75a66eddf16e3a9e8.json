\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport qs from \"qs\";\r\nimport { getMoney } from \"@/api/common\";\r\nimport jsCookie from 'js-cookie';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      money: \"\",\r\n      cardno: jsCookie.get('cardnumber'),\r\n      password: jsCookie.get('password'),\r\n    };\r\n  },\r\n\r\n  created() {\r\n    var id = JSON.parse(sessionStorage.getItem(\"user\")).id;\r\n    var params = {\r\n      id: id,\r\n    };\r\n    var data = qs.stringify(params);\r\n    this.$axios\r\n      .post(\"http://127.0.0.1:8080/BankTransfer/getCardNo\", data)\r\n      .then((res) => {\r\n        this.cardno = res.data;\r\n      });\r\n  },\r\n\r\n  methods: {\r\n    withdraw() {\r\n      if (\r\n        this.password.trim() ==\r\n          JSON.parse(sessionStorage.getItem(\"user\")).password && this.money.trim() > 0\r\n      ) {\r\n        this.money = this.money.trim();\r\n        if (this.$store.state.money - this.money < 0) {\r\n          this.$message({\r\n            showClose: true,\r\n            message: \"余额不足\",\r\n            type: \"warning\",\r\n          });\r\n        } else{\r\n                  var id = JSON.parse(sessionStorage.getItem(\"user\")).id;\r\n        var params = {\r\n          cardno: this.cardno,\r\n          money: this.money,\r\n          id: id,\r\n        };\r\n        var data = qs.stringify(params);\r\n        var _this = this;\r\n        this.$axios\r\n          .post(\"http://127.0.0.1:8080/BankTransfer/withdraw\", data)\r\n          .then(\r\n            function(res) {\r\n              console.log(res.data);\r\n              this.$message({\r\n                showClose: true,\r\n                message: \"取款成功\",\r\n                type: \"success\",\r\n              });\r\n              getMoney(this);\r\n              this.$router.push(\"/WithdrawSuccess\");\r\n            }.bind(_this)\r\n          )\r\n          .catch((err) => {\r\n            this.$message({\r\n              showClose: true,\r\n              message: \"请重试\",\r\n              type: \"error\",\r\n            });\r\n          });\r\n        }\r\n      } else {\r\n        this.$message({\r\n          showClose: true,\r\n          message: \"请重试\",\r\n          type: \"error\",\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style >\r\n.main {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  width: 100%;\r\n  height: 750px;\r\n  background: url(\"../../assets/中国银行图片.png\") no-repeat center fixed;\r\n  background-size: cover;\r\n}\r\n.left {\r\n  padding-top: 300px;\r\n  box-sizing: border-box;\r\n  width: 300px;\r\n  height: 750px;\r\n}\r\n.center {\r\n  padding-top: 50px;\r\n  box-sizing: border-box;\r\n  width: 900px;\r\n  height: 750px;\r\n  text-align: center;\r\n}\r\n.right {\r\n  padding-top: 300px;\r\n  box-sizing: border-box;\r\n  text-align: right;\r\n  width: 300px;\r\n  height: 750px;\r\n}\r\n.buttonDistance {\r\n  padding-top: 20px;\r\n  box-sizing: border-box;\r\n}\r\n</style>"],"mappings":";AAsBA,OAAAA,EAAA;AACA,SAAAC,QAAA;AACA,OAAAC,QAAA;AAEA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,MAAA,EAAAH,QAAA,CAAAI,GAAA;MACAC,QAAA,EAAAL,QAAA,CAAAI,GAAA;IACA;EACA;EAEAE,QAAA;IACA,IAAAC,EAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,OAAA,UAAAJ,EAAA;IACA,IAAAK,MAAA;MACAL,EAAA,EAAAA;IACA;IACA,IAAAN,IAAA,GAAAH,EAAA,CAAAe,SAAA,CAAAD,MAAA;IACA,KAAAE,MAAA,CACAC,IAAA,iDAAAd,IAAA,EACAe,IAAA,CAAAC,GAAA;MACA,KAAAd,MAAA,GAAAc,GAAA,CAAAhB,IAAA;IACA;EACA;EAEAiB,OAAA;IACAC,SAAA;MACA,IACA,KAAAd,QAAA,CAAAe,IAAA,MACAZ,IAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,OAAA,UAAAN,QAAA,SAAAH,KAAA,CAAAkB,IAAA,QACA;QACA,KAAAlB,KAAA,QAAAA,KAAA,CAAAkB,IAAA;QACA,SAAAC,MAAA,CAAAC,KAAA,CAAApB,KAAA,QAAAA,KAAA;UACA,KAAAqB,QAAA;YACAC,SAAA;YACAC,OAAA;YACAC,IAAA;UACA;QACA;UACA,IAAAnB,EAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,OAAA,UAAAJ,EAAA;UACA,IAAAK,MAAA;YACAT,MAAA,OAAAA,MAAA;YACAD,KAAA,OAAAA,KAAA;YACAK,EAAA,EAAAA;UACA;UACA,IAAAN,IAAA,GAAAH,EAAA,CAAAe,SAAA,CAAAD,MAAA;UACA,IAAAe,KAAA;UACA,KAAAb,MAAA,C