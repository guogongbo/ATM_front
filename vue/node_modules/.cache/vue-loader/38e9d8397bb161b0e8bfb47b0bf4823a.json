取款金额\"></el-input>\r\n      <el-card shadow=\"always\">账户余额：{{ $store.state.money }} 元</el-card>\r\n    </div>\r\n    <div class=\"right\">\r\n      <div class=\"buttonDistance\">\r\n          <el-button plain @click=\"withdraw\">确认</el-button>\r\n      </div>\r\n      <div class=\"buttonDistance\">\r\n        <router-link to=\"/WithdrawView\">\r\n          <el-button>返回</el-button>\r\n        </router-link>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport qs from \"qs\";\r\nimport { getMoney } from \"@/api/common\";\r\nimport jsCookie from 'js-cookie';\r\nlet sumqumoney = 0;\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      money: \"\",\r\n      cardno: jsCookie.get('cardnumber'),\r\n      password: jsCookie.get('password'),\r\n    };\r\n  },\r\n\r\n  created() {\r\n    var id = JSON.parse(sessionStorage.getItem(\"user\")).id;\r\n    var params = {\r\n      id: id,\r\n    };\r\n    var data = qs.stringify(params);\r\n    // this.$axios\r\n    //   .post(\"http://127.0.0.1:8080/BankTransfer/getCardNo\", data)\r\n    //   .then((res) => {\r\n    //     this.cardno = res.data;\r\n    //   });\r\n\r\n    this.$axios\r\n      .post(\"http://127.0.0.1:8080/BankTransfer/getRecords\", data)\r\n      .then((res) => {\r\n        this.dealResponse(res.data);\r\n      });\r\n  },\r\n\r\n  methods: {\r\n    dealResponse(arr) {\r\n      sumqumoney = 0;\r\n      for (let o of arr) {\r\n        let to = {};\r\n        to.record_id = o.id;\r\n        to.cardno = o.card.cardno;\r\n        to.amount = o.amount;\r\n        to.type = o.type;\r\n        if (o.type == 0) {\r\n          to.type = \"转账支出\";\r\n        } else if (o.type == 1) {\r\n          to.type = \"存入\";\r\n        } else {\r\n          to.type = \"取款支出\";\r\n        }\r\n        to.date = 123;\r\n        var d = new Date(o.date);\r\n        to.date = `${d.getFullYear()}-${d.getMonth()}-${d.getDate()} ${d.getHours()}:${d.getMinutes()}:${d.getSeconds()}`;\r\n        let date = new Date();\r\n        if(d.getFullYear() == date.getFullYear()&&d.getMonth() == date.getMonth()&&d.getDate() == date.getDate()){\r\n          if(o.type == 2){\r\n            sumqumoney += o.amount;\r\n            console.log(\"今日取款\" + sumqumoney);\r\n          }\r\n        }\r\n      }\r\n    },\r\n\r\n    withdraw() {\r\n      var id = JSON.parse(sessionStorage.getItem(\"user\")).id;\r\n      var params = {\r\n        id: id,\r\n      };\r\n      var data = qs.stringify(params);\r\n\r\n      this.$axios\r\n      .post(\"http://127.0.0.1:8080/BankTransfer/getRecords\", data)\r\n      .then((res) => {\r\n        this.dealResponse(res.data);\r\n      });\r\n\r\n      if (\r\n        this.password.trim() ==\r\n          JSON.parse(sessionStorage.getItem(\"user\")).password && this.money.trim() > 0\r\n      ){\r\n        this.money = this.money.trim();\r\n        if(this.money>2500){\r\n        \r\n            this.$message({\r\n            showClose: true,\r\n            message: \"单次取款金额超过限额2500，无法取款，请重试\",\r\n            type: \"warning\",});\r\n            return;\r\n        }\r\n        if(sumqumoney +parseInt( this.money) > 20000) {\r\n          this.$message({\r\n            showClose: true,\r\n            message: \"取款金额超过今日限额20000,无法取款,请重试\",\r\n            type: \"warning\",\r\n          });\r\n          return;\r\n        }\r\n          if(this.money%100!=0){\r\n          this.$message({\r\n            showClose: true,\r\n            message: \"所取金额面值必须为100\",\r\n            type: \"warning\",});\r\n        }\r\n        else if (this.$store.state.money - this.money < 0) {\r\n          this.$message({\r\n            showClose: true,\r\n            message: \"余额不足\",\r\n            type: \"warning\",\r\n          });\r\n        }\r\n        else{\r\n                  var id = JSON.parse(sessionStorage.getItem(\"user\")).id;\r\n        var params = {\r\n          cardno: this.cardno,\r\n          money: this.money,\r\n          id: id,\r\n        };\r\n        var data = qs.stringify(params);\r\n        var _this = this;\r\n        this.$axios\r\n          .post(\"http://127.0.0.1:8080/BankTransfer/withdraw\", data)\r\n          .then(\r\n            function(res) {\r\n              console.log(res.data);\r\n              this.$message({\r\n                showClose: true,\r\n                message: \"取款成功\",\r\n                type: \"success\",\r\n              });\r\n               jsCookie.set(\"type\", \"取款\");\r\n      jsCookie.set(\"transactionDetail\", this.money);\r\n              getMoney(this);\r\n              this.$router.push(\"/WithdrawSuccess\");\r\n            }.bind(_this)\r\n          )\r\n          .catch((err) => {\r\n            this.$message({\r\n              showClose: true,\r\n              message: \"请重试\",\r\n              type: \"error\",\r\n            });\r\n          });\r\n        }\r\n      } else {\r\n        this.$message({\r\n          showClose: true,\r\n          message: \"所取金额不能为0\",\r\n          type: \"error\",\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style >\r\n.main {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  width: 100%;\r\n  height: 780px;\r\n  background: url(\"../../assets/中国银行图片.png\") no-repeat center fixed;\r\n  background-size: cover;\r\n}\r\n.left {\r\n  padding-top: 300px;\r\n  box-sizing: border-box;\r\n  width: 300px;\r\n  height: 750px;\r\n}\r\n.center {\r\n  padding-top: 50px;\r\n  box-sizing: border-box;\r\n  width: 900px;\r\n  height: 750px;\r\n  text-align: center;\r\n}\r\n.right {\r\n  padding-top: 300px;\r\n  box-sizing: border-box;\r\n  text-align: right;\r\n  width: 300px;\r\n  height: 750px;\r\n}\r\n.buttonDistance {\r\n  padding-top: 20px;\r\n  box-sizing: border-box;\r\n}\r\n.el-button{\r\n  font-size: 30px;\r\n  color:black;\r\n  background-color: white;\r\n}\r\n</style>"]}]}